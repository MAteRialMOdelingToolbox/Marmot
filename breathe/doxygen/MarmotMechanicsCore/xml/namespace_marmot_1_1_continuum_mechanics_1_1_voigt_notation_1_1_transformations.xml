        <name>projectVoigtStrainToPlane</name>
        <qualifiedname>Marmot::ContinuumMechanics::VoigtNotation::Transformations::projectVoigtStrainToPlane</qualifiedname>
        <param>
          <type>const Vector3d &amp;</type>
