        <qualifiedname>Marmot::Materials::LinearElastic::computeStress</qualifiedname>
        <reimplements refid="classMarmotMaterialHypoElastic_1a4751feeb33fc3205fc5c2b75fdc0d30c">computeStress</reimplements>
